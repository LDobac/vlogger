{
  "name": "vlogger",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:server": "vite build --outDir ssg/server --ssr src/ssr/entry-server.js",
    "generate": "vite build --ssrManifest --outDir ssg/static && yarn build:server && node src/ssr/prerender.js",
    "serve": "vite preview",
    "posts": "node ./scripts/BuildPosts.js",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore --fix src",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^4.3.1",
    "dayjs": "^1.11.8",
    "express": "^4.18.2",
    "github-markdown-css": "^5.2.0",
    "highlight.js": "^11.8.0",
    "vite": "^4.4.9",
    "vue": "^3.3.4",
    "vue-gtag": "^2.0.1",
    "vue-router": "^4.2.2",
    "vue-tsc": "^1.8.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "compression": "^1.7.4",
    "eslint": "^8.42.0",
    "eslint-plugin-vue": "^9.14.1",
    "fs-extra": "^11.1.1",
    "gh-pages": "^5.0.0",
    "marked": "^5.1.0",
    "node-gyp": "^9.4.0",
    "replace-ext": "^2.0.0",
    "sass": "^1.63.3",
    "serve-static": "^1.15.0",
    "typescript": "^5.1.0",
    "vue-jest": "^5.0.0-alpha.10",
    "yaml": "^2.3.1"
  },
  "packageManager": "yarn@3.6.0"
}
